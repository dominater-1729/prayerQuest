<head>
    {{>websiteHeader}}
    {{{body}}}
</head>
<section id="listing" class="pt-50 pb-50">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <ul class="ps-0  d-flex flex-wrap gap-2  align-items-center pb-50" style="list-style: none;">
                    <li class="text-yeloish">Home</li>
                    <li>></li>
                    <li class="text-yeloish">Group Profile</li>
                    <li>></li>
                    <li>Members</li>

                </ul>

            </div>

            <div class="col-lg-12">
                <div class="listing d-flex justify-content-between align-items-center flex-wrap gap-2">
                    <div>
                        <h3 class="mb-0 fw-bold">Members Listings</h3>
                        <p class="mb-0 text-black-50">( Showing {{inc members.length}} Results )
                        </p>
                    </div>
                    <div class="search d-flex gap-3">
                        <form action="" class="position-relative d-flex gap-3" id="search-list">
                            <div>
                                <input type="text" class="form-control form-custom ps-3" style="width: 320px;"
                                    placeholder="Search...">
                                <a href="#" class="btn btn-danger search-icon"
                                    style="right: 0;
                                        padding: 10px 18px;
                                        margin-top: 0px;
                                        border-top-left-radius: 0px !important;border-bottom-left-radius: 0px !important;border-radius: 16px;"><i
                                        class="fa fa-search"></i></a>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 mt-5">
                <div class="row">
                    <div class="col-lg-3 col-md-4 col-sm-6 col-12 mb-7">
                        <div class="card br-16" style="height: 380px;">
                            <div class="member-cover w-100">
                                <img src="/assets/images/groupImage/{{decryptImage groupImage}}" alt=""
                                    class="w-100 h-160px">
                            </div>
                            <div class="member-profile text-center pb-5">
                                <img src="/assets/images/userImage/{{decryption admin.image}}" alt=""
                                    class="rounded-circle mb-2" width="100" height="100">
                                <h5 class="mb-0 fs-18 fw-600 pb-2">{{decryption admin.firstname}} {{decryption
                                    admin.lastname}}</h5>
                                <p class="badge mb-0 custom-badge br-16">Admin</p>
                            </div>
                        </div>
                    </div>
                    {{#each members}}
                    <div class="col-lg-3 col-md-4 col-sm-6 col-12 mb-7">
                        <div class="card br-16" style="height: 380px;">
                            <div class="member-cover w-100">
                                <img src="/assets/images/userImage/{{../decryption image}}" alt=""
                                    class="w-100 h-160px">
                            </div>
                            <div class="member-profile text-center pb-5">
                                <img src="/assets/images/userImage/{{../decryption image}}" alt=""
                                    class="rounded-circle mb-2" width="100" height="100">
                                <h5 class="mb-0 fs-18 fw-600 pb-2">{{../decryption firstname}} {{../decryption
                                    lastname}}</h5>
                                <p class="badge mb-0 custom-badge br-16">Member</p>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</section>

{{>websiteFooter}} {{{body}}}